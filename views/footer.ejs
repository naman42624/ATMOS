<script>
  AOS.init(
    {
      offset: 400,
      duration: 1000,
    }
  );
</script>
<script>
  window.addEventListener('scroll', () => {
    var elements = document.querySelectorAll('.show-on-scroll');
    for (var i = 0; i < elements.length; i++) {
      var element = elements[i];
      var rect = element.getBoundingClientRect();
      var isVisible = rect.left >= 0 && rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) && rect.right <= (window.innerWidth || document.documentElement.clientWidth);
      if (isVisible) {
        element.classList.add('active');
      } else {
        element.classList.remove('active');
      }
    }
    var elements = document.querySelectorAll('.nav');
    for (var i = 0; i < elements.length; i++) {
      var element = elements[i];
      if (scrollY > 50) {
        element.classList.add('solid');
      } else {
        element.classList.remove('solid');
      }
    }
  });

</script>
